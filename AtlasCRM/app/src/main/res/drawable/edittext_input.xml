<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true">
        <shape >
            <gradient android:startColor="@color/white_color" android:endColor="@color/white_color" android:angle="90"/>
            <stroke android:width="0.6dp" android:color="@color/white_color"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape >
            <gradient android:startColor="@color/white_color" android:endColor="@color/white_color" android:angle="90"/>
            <stroke android:width="0.6dp" android:color="@color/white_color"/>
            <corners android:radius="5dp"/>
        </shape>
    </item>
    <item >
        <shape>
            <gradient android:startColor="@color/white_color" android:endColor="@color/white_color" android:angle="90"/>
            <stroke android:width="0.6dp" android:color="@color/white_color"
                />
            <corners android:radius="5dp" />
        </shape>
    </item>
</selector>